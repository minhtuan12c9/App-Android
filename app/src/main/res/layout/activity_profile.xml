<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".NormalTest.ProfileActivity">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="423dp"
        android:layout_height="50dp"
        android:text="USER PROFILE"
        android:gravity="center_vertical"
        android:paddingLeft="30dp"
        android:textSize="20dp"
        android:textStyle="bold"
        android:textColor="@color/white"
        android:background="@android:color/holo_blue_dark"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="400dp"
        android:layout_height="132dp"
        android:layout_marginTop="28dp"
        android:src="@drawable/android"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <EditText
        android:id="@+id/hoten"
        android:layout_width="344dp"
        android:layout_height="61dp"
        android:layout_marginTop="28dp"
        android:ems="10"
        android:hint="Nhập họ tên"
        android:inputType="text"
        android:text=""
        android:focusable="false"
        android:clickable="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <EditText
        android:id="@+id/sdt"
        android:layout_width="344dp"
        android:layout_height="61dp"
        android:layout_marginTop="112dp"
        android:ems="10"
        android:hint="Nhập số điện thoại"
        android:inputType="text"
        android:text=""
        android:focusable="false"
        android:clickable="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="157dp"
        android:layout_height="38dp"
        android:layout_marginTop="52dp"
        android:text="Chọn ngành học"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.129"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sdt" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="255dp"
        android:layout_height="41dp"
        android:layout_marginTop="112dp"
        android:text="Các ngôn ngữ đã biết"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.211"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sdt" />

    <Button
        android:id="@+id/chonNganh"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="109dp"
        android:layout_height="49dp"
        android:layout_marginTop="40dp"
        android:text="Chọn"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.887"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sdt" />

    <Button
        android:id="@+id/chonNgonNgu"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="109dp"
        android:layout_height="49dp"
        android:layout_marginTop="96dp"
        android:text="Chọn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.887"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sdt" />

    <Button
        android:id="@+id/thoat"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="109dp"
        android:layout_height="49dp"
        android:layout_marginTop="284dp"
        android:text="Thoát"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sdt" />

    <Button
        android:id="@+id/dangKy"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="109dp"
        android:layout_height="49dp"
        android:layout_marginTop="284dp"
        android:text="Đăng ký"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.897"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sdt" />

    <TextView
        android:id="@+id/ketqua"
        android:layout_width="349dp"
        android:layout_height="105dp"
        android:layout_marginTop="164dp"
        android:background="@color/tieude2"
        android:paddingHorizontal="10dp"
        android:paddingVertical="10dp"
        android:text=""
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sdt" />

    <TextView
        android:id="@+id/ketqua1"
        android:layout_width="346dp"
        android:layout_height="41dp"
        android:layout_marginTop="176dp"
        android:background="@color/tieude2"
        android:paddingHorizontal="10dp"

        android:text=""
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sdt" />

    <TextView
        android:id="@+id/ketqua2"
        android:layout_width="344dp"
        android:layout_height="41dp"
        android:layout_marginTop="220dp"
        android:background="@color/tieude2"
        android:paddingHorizontal="10dp"

        android:text=""
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.477"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sdt" />

</androidx.constraintlayout.widget.ConstraintLayout>